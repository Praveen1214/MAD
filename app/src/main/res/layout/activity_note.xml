<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Modules.note">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="386dp"
        android:layout_height="63dp"
        app:layout_constraintBottom_toBottomOf="@+id/bottomNavigation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.36"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.12"
        app:srcCompat="@drawable/calendar" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigation"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:labelVisibilityMode="unlabeled"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_nav_bar" />


    <RelativeLayout
        android:id="@+id/assignment_"
        android:layout_width="wrap_content"
        android:layout_height="72dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="80dp"
        android:layout_marginBottom="36dp"

        app:layout_constraintBottom_toTopOf="@+id/assignment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView12"
        tools:ignore="RtlHardcoded">

        <CheckBox
            android:id="@+id/checkBox2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="300dp"
            android:layout_marginTop="15dp"
            android:button="@drawable/download"
            android:gravity="center|center_horizontal"
            tools:ignore="DuplicateSpeakableTextCheck" />

        <View
            android:id="@+id/ellipse_5"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:background="#79E0EE" />

        <TextView
            android:id="@+id/problem_sol"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="11dp"
            android:gravity="top"
            android:text="@string/problem_sol"
            android:textAppearance="@style/problem_sol"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/cryptograph"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginTop="40dp"
            android:gravity="top"
            android:text="@string/cryptograph"
            android:textAppearance="@style/cryptograph"
            android:textSize="14sp" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/assignment1"
        android:layout_width="wrap_content"
        android:layout_height="72dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="35dp"
        android:background="@drawable/component_9"
        app:layout_constraintBottom_toBottomOf="@+id/bottomNavigation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/assignment"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="UnknownId">

        <CheckBox
            android:id="@+id/checkBox3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="300dp"
            android:layout_marginTop="15dp"
            android:button="@drawable/download" />

        <View
            android:id="@+id/ellipse_7"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:background="#79E0EE" />

        <TextView
            android:id="@+id/problem_sol2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginTop="11dp"
            android:gravity="top"
            android:text="@string/problem_sol"
            android:textAppearance="@style/problem_sol"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/cryptograph2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginTop="40dp"
            android:gravity="top"
            android:text="@string/cryptograph"
            android:textAppearance="@style/cryptograph"
            android:textSize="14sp" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/assignment"
        android:layout_width="match_parent"
        android:layout_height="72dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/component_9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/assignment_">

        <View
            android:id="@+id/ellipse_6"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:background="#79E0EE" />

        <TextView
            android:id="@+id/problem_sol1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginTop="11dp"
            android:gravity="top"
            android:text="@string/problem_sol"
            android:textAppearance="@style/problem_sol"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/cryptograph1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginTop="40dp"
            android:gravity="top"
            android:text="@string/cryptograph"
            android:textAppearance="@style/cryptograph"
            android:textSize="14sp" />

        <CheckBox
            android:id="@+id/checkBox4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="300dp"
            android:layout_marginTop="15dp"
            android:button="@drawable/download" />


    </RelativeLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/boxtime"
            android:layout_width="18dp"
            android:layout_height="55dp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:contentDescription="TODO"
            android:src="@drawable/logo"
            tools:srcCompat="@drawable/frame_433" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_weight="2"
            android:background="#FFFFFF"
            android:gravity="center"
            android:text="@string/notes"
            android:textAlignment="center"
            android:textColor="#1B1B1B"
            android:textSize="20sp"
            android:textStyle="bold" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="35dp"
            android:layout_height="55dp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:contentDescription="@string/todo33"
            android:src="@drawable/search"
            tools:ignore="ImageContrastCheck,DuplicateSpeakableTextCheck"
            tools:src="@drawable/search" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>